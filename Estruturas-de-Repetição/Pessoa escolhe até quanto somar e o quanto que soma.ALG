Algoritmo "semnome"
Var
   contador, valor, soma: inteiro
Inicio
   contador <- 0
   Escreva ("Contar até quanto?: ")
   Leia (valor)
   Escreva ("Somar quanto?: ")
   Leia (soma)
   Enquanto (contador <= valor) faca
      Escreval (contador)
      contador <- contador + soma
   FimEnquanto
Fimalgoritmo